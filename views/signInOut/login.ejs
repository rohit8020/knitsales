<body>   
<%- include('../partials/nav'); -%>


<div class="form-container">
    <div class="form">
        <h1 class="heading">LogIn <i class="fa fa-sign-in" aria-hidden="true"></i></h1>
        <form action="/login" method="POST">
            <input class="form-control" type="text" name="username" placeholder="username" required>
            <input id="password" 
            class="form-control" type="password" name="password" placeholder="password" required>
            <label class="btn" for="submit">Login</label>
            <button id="submit" hidden></button>
        </form>
    </div>     
</div>

